{
	"name": "aws-ssm-parameter-manager",
	"productName": "Parameters",
	"version": "0.6.0",
	"description": "Easily manage AWS SSM Parameter Store items across regions",
	"main": "src/main.js",
	"scripts": {
		"start": "electron-forge start",
		"dev": "electron . --debug",
		"debug": "electron . --debug",
		"test": "echo \"Error: no test specified\" && exit 1",
		"package": "electron-forge package",
		"make": "electron-forge make"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/observian/aws-ssm-parameter-manager.git"
	},
	"keywords": [
		"Observian",
		"SSM",
		"AWS",
		"Parameters"
	],
	"author": "Jaaromy Zierse",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/observian/aws-ssm-parameter-manager/issues"
	},
	"homepage": "https://github.com/observian/aws-ssm-parameter-manager#readme",
	"dependencies": {
		"ag-grid": "^17.1.1",
		"ag-grid-react": "^17.1.0",
		"aws-sdk": "^2.236.1",
		"bluebird": "^3.5.1",
		"clone": "^2.1.1",
		"electron-compile": "^6.4.2",
		"electron-devtools-installer": "^2.2.4",
		"electron-squirrel-startup": "^1.0.0",
		"fast-deep-equal": "^2.0.1",
		"line-by-line": "^0.1.6",
		"mkdirp": "^0.5.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-dom-factories": "^1.0.2",
		"react-fontawesome": "^1.6.1",
		"react-hot-loader": "^4.1.3",
		"react-redux": "^5.0.7",
		"react-select": "^1.2.1",
		"redux": "^4.0.0",
		"redux-electron-store": "^0.6.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-plugin-transform-es2015-classes": "^6.24.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"electron-forge": "^5.2.1",
		"electron-prebuilt-compile": "1.8.4",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0"
	},
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"zip",
					"dmg"
				],
				"linux": [
					"deb",
					"flatpak"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm",
				"appBundleId": "A525J3CKM8.com.github.aws-ssm-parameter-manager",
				"osxSign": true
			},
			"electronWinstallerConfig": {
				"name": "Parameters"
			},
			"electronInstallerDebian": {
				"name": "Parameters"
			},
			"electronInstallerFlatpak": {
				"name": "Parameters"
			},
			"electronInstallerRedhat": {
				"name": "Parameters"
			},
			"github_repository": {
				"owner": "observian",
				"name": "aws-ssm-parameter-manager"
			},
			"windowsStoreConfig": {
				"packageName": "",
				"name": "ObservianParameterManager"
			}
		}
	}
