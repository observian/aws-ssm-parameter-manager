{
	"name": "aws-ssm-parameter-manager",
	"productName": "Parameters",
	"version": "0.5.2",
	"description": "Easily manage AWS SSM Parameter Store items across regions",
	"main": "src/main.js",
	"scripts": {
		"start": "electron-forge start",
		"dev": "electron . --debug",
		"debug": "electron . --debug",
		"test": "echo \"Error: no test specified\" && exit 1",
		"package": "electron-forge package",
		"make": "electron-forge make"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/observian/aws-ssm-parameter-manager.git"
	},
	"keywords": [
		"Observian",
		"SSM",
		"AWS",
		"Parameters"
	],
	"author": "Jaaromy Zierse",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/observian/aws-ssm-parameter-manager/issues"
	},
	"homepage": "https://github.com/observian/aws-ssm-parameter-manager#readme",
	"dependencies": {
		"ag-grid": "^17.0.0",
		"aws-sdk": "2.220.1",
		"bluebird": "3.5.1",
		"electron-compile": "^6.4.2",
		"electron-squirrel-startup": "^1.0.0",
		"glob": "^7.1.2",
		"jquery": "^3.3.1",
		"query-string": "^6.0.0"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"devtron": "1.4.0",
		"electron-forge": "^5.1.1",
		"electron-prebuilt-compile": "1.8.4",
		"transpile": "^2.6.2"
	},
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"zip",
					"dmg"
				],
				"linux": [
					"deb",
					"flatpak"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm",
				"appBundleId": "A525J3CKM8.com.github.aws-ssm-parameter-manager",
				"osxSign": true
			},
			"electronWinstallerConfig": {
				"name": "Parameters"
			},
			"electronInstallerDebian": {
				"name": "Parameters"
			},
			"electronInstallerFlatpak": {
				"name": "Parameters"
			},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "observian",
				"name": "aws-ssm-parameter-manager"
			},
			"windowsStoreConfig": {
				"packageName": "",
				"name": "ObservianParameterManager"
			}
		}
	}
}
